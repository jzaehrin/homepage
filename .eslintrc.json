{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-param-reassign": 0,
        "no-console": [
            2, 
            { 
                "allow": ["debug", "error"] 
            }
        ],
        "react/jsx-filename-extension": [
            2,
            {
                "extensions": [
                    ".js"
                ]
            }
        ]
    }
}
